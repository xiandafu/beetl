<div class="dddf">

          <div>
            <div class="name"> 出版年代从&nbsp;
              <select name="startYear" style="width:90px;">
                <option value="">请选择</option>
                <%--
                  var curDate = date();
                  var year = strutil.formatDate(curDate,"yyyy");
                  var i = parseInt(year);
                  while(i >= 1980 ){
                --%>
                <option value="${i}" ${(parameter.startYear!''==''+i)}>${i}年</option>
                <%--i--; }--%>
              </select>
              到
              <select name="endYear" style="width:150px;">
                <option value="">请先选择开始年代</option>
                <%--
                  curDate = date();
                  year = strutil.formatDate(curDate,"yyyy");
                  i = parseInt(year);
                  while(i >= 1980 ){
                --%>
                <option value="${i}" ${parameter.endYear!''==''+i?'selected':''}>${i}年</option>
                <%--i--; }--%>
              </select></div>
          </div>
</div>
